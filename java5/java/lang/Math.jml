      @ also
      @   requires (isNegativeZero(y) && x < 0.0)
      @         || (Double.NEGATIVE_INFINITY < y && y < 0.0
      @             && x == Double.NEGATIVE_INFINITY);
      @   ensures \result == -PI;
      @ also
      @   requires (y > 0.0 && x == 0.0)
      @         || (y == Double.POSITIVE_INFINITY
      @             && Double.NEGATIVE_INFINITY < x

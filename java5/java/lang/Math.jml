      @   ensures \result == -PI/2.0;
      @ also
      @   requires y == Double.POSITIVE_INFINITY 
      @         && x == Double.POSITIVE_INFINITY;
      @   ensures \result == PI/4.0;
      @ also
      @   requires y == Double.POSITIVE_INFINITY
      @         && x == Double.NEGATIVE_INFINITY;
      @   ensures \result == 3.0*PI/4.0;
      @ also
      @   requires y == Double.NEGATIVE_INFINITY
